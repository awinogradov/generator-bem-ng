{
    "name" : "<%= _.slugify(appname) %>",
    "version" : "0.0.1",
    "cwd" : ".",
    "projectLevels" : [
        "app.common.blocks",
        "app.desktop.blocks",
        "app.touch.blocks"
    ],
    "projectBundles" : [
        "app.desktop.bundles"
    ],
    "assets" : [
        "app.assets"
    ],
    "libDir" : "bower_components",
    "libLevels" : [ 
        "bem-core/common.blocks",
        "bem-core/desktop.blocks",
        "bem-components/common.blocks",
        "bem-components/desktop.blocks"<% if (includeExamples) { %>,
        "bem-example/example.blocks"<% } %>
    ],
    "dist" : "dist",
    "tmp" : ".tmp",
    "favicon" : "favicon.ico",
    "styles" : "dist/styles",
    "scripts" : "dist/scripts",
    "env" : "dev",
    "compress" : false,
    "comments" : true
}
