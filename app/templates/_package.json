{
    "private": true,
    "name": "<%= _.slugify(appname) %>",
    "version": "0.0.0",
    "description": "Description for <%= _.slugify(appname) %>",
    "homepage": "https://github.com/<%= username %>/<%= _.slugify(appname) %>",
    "bugs": "https://github.com/<%= username %>/<%= _.slugify(appname) %>/issues",
    "author": {
        "name": "<%= username %>",
        "email": "<%= email %>",
        "url": "https://github.com/<%= username %>"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/<%= username %>/<%= _.slugify(appname) %>.git"
    },
    "_directories": {
        "app": "app",
        "server": "server",
        "blocks": "*.blocks",
        "bundles": "*.bundles",
        "libs": "libs",
        "public": "public",
        "styles": "assets",
        "scripts": "assets"
    },
    "_settings": {
        "env": "development"
    },
    "dependencies": {
        "bem-environ": "~1.4.0",
        "roole": "~0.9.0",
        "bem-tools-autoprefixer": "~0.0.3"
    },
    "devDependencies": {
        "bem": "~0.8.1",
        "bower": "~1.2.8",
        "borschik": "~0.4.2",
        "bower-npm-install": "~0.5.2",
        "bem-cli": "~1.0.1",
        "gulp": "~3.6.0",
        "gulp-if": "0.0.5",
        "gulp-jshint": "~1.5.0",
        "gulp-csso": "~0.2.7",
        "gulp-rename": "~1.2.0",
        "gulp-notify": "~1.2.5",
        "gulp-concat": "~2.2.0"
    },
    "scripts": {
        "postinstall": "bower-npm-install --non-interactive",
        "deps": "bower-npm-install --non-interactive"
    },
    "engines": {
        "node": ">=0.10.0"
    },
    "license": "MIT"
}
