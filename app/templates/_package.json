{
    "private": true,
    "name": "<%= _.slugify(appname) %>",
    "version": "0.0.0",
    "description": "Description for <%= _.slugify(appname) %>",
    "homepage": "https://github.com/<%= username %>/<%= _.slugify(appname) %>",
    "bugs": "https://github.com/<%= username %>/<%= _.slugify(appname) %>/issues",
    "author": {
        "name": "<%= username %>",
        "email": "<%= email %>",
        "url": "https://github.com/<%= username %>"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/<%= username %>/<%= _.slugify(appname) %>.git"
    },
    "_directories": {
        "public": "public",
        "styles": "assets",
        "scripts": "assets"
    },
    "dependencies": {
        "bem-environ": "~1.4.0",
    },
    "devDependencies": {
        "bem": "0.9.0",
        "bem-cli": "1.0.1",
        "bem-tools-autoprefixer": "0.0.3",
        "borschik": "1.3.0",
        "bower": "1.3.11",
        "bower-npm-install": "0.5.8",
        "gulp": "3.8.8",
        "gulp-concat": "2.4.1",
        "gulp-clean-css": "2.4.1",
        "gulp-filter": "1.0.2",
        "gulp-if": "1.2.4",
        "gulp-jshint": "1.8.4",
        "gulp-load-plugins": "0.6.0",
        "gulp-nodemon": "1.0.4",
        "gulp-notify": "1.7.0",
        "gulp-rename": "1.2.0",
        "gulp-shell": "0.2.9",
        "run-sequence": "0.3.9",
        "stylus": "0.49.0",
        "browser-sync": "1.5.1"
    },
    "scripts": {
        "postinstall": "bower-npm-install",
        "deps": "bower-npm-install --non-interactive"
    },
    "engines": {
        "node": ">=0.10"
    },
    "license": "MIT"
}
