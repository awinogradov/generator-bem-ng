{
    "public": "./public",
    "assets": {
        "name": "assets",
        "dir": "./assets",
        "cwd": "./public/assets",
        "styles": {
            "name": "application.min.css",
            "dir": "./public/assets"
        },
        "scripts": {
            "name": "application.min.js",
            "dir": "./public/assets"
        }
    },
    "browsers_support": [
        "last 2 versions",
        "ie 10",
        "ff 24",
        "opera 12.16",
        "android 4",
        "ios 6"
    ],
    "service" : {
        "levels" : [ "common.blocks", "service.blocks", "application.blocks" ],
        "pages" : {
            "dir" : "service.pages"
        }
    },
    "server" : {
        "deps" : [ "express", "body-parser", "errorhandler", "morgan", "winston", "consolidate", "ejs" ],
        "env"  : "development",
        "port" : 3000
    }
}
